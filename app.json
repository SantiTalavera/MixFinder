{
  "expo": {
    "name": "MixFinder Mobile",
    "slug": "mixfinder-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1DB954"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mixfinder.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1DB954"
      },
      "package": "com.mixfinder.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "mixfinder",
    "plugins": [
      "expo-secure-store",
      "expo-web-browser"
    ]
  }
}
